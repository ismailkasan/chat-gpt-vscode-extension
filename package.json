{
  "name": "vscode-chat-gpt",
  "displayName": "vscode-chat-gpt",
  "description": "Extension is a chat bot that uses ChatGpt Api.",
  "version": "0.0.1",
  "publisher": "ikasann-self",
  "bugs": {
    "url": "https://github.com/ismailkasan/chat-gpt-vscode-extension/issues",
    "email": "ismailkasan63@gmail.com"
  },
  "homepage": "https://github.com/ismailkasan/chat-gpt-vscode-extension/main/README.md",
  "repository": {
    "url": "https://github.com/ismailkasan/chat-gpt-vscode-extension",
    "type": "git"
  },
  "keywords": [
    "openai",
    "chatgpt",
    "gpt",
    "chat",
    "ai"
  ],
  "icon": "out/chat-gpt-logo-2-HBRQ6ZBV.jpeg",
  "galleryBanner": {
    "color": "#0000FF",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
			"activitybar": [
				{
					"id": "chat-gpt-view",
					"title": "Chat Gpt",
					"icon": "src/images/activity-bar-logo.svg"
				}
			]
		},
    "views": {
			"chat-gpt-view": [
				{
					"id": "chat-gpt-view-id",
					"name": "Chat Gpt View",
					"icon": "src/images/activity-bar-logo.svg",
					"contextualTitle": "Chat Gpt view-2"
				}
			]		
		},
    "viewsWelcome": [
      {
        "view": "chat-gpt-view-id",
        "contents": "More information about OpenAi and ChatGpt [please visit](https://openai.com/).\n[Open Chat Editor](command:vscode-chat-gpt.start)"
      }
    ],
    "commands": [
      {
        "command": "vscode-chat-gpt.start",
        "title": "Ask to ChatGpt"
      }
    ],
    "webviews": [
      {
        "viewType": "markdown.preview",
        "icon": {
          "light": "./out/chat-gpt-logo-2-HBRQ6ZBV.jpeg",
          "dark": "./out/chat-gpt-logo-2-HBRQ6ZBV.jpeg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@types/vscode-webview": "^1.57.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "rxjs": "^7.8.0",
    "undici": "^5.21.0"
  }
}